@using System.Security.Claims

@{
    ViewData["Title"] = "Authorized";
}

<h2>Authorized</h2>

<p>If you can see this page, you're logged in.</p>

<h3>Claims</h3>

<table class="table-hover table-condensed table-striped">
    <tr>
        <th>Claim Type</th>
        <th>Claim Value</th>
    </tr>

    @foreach (Claim claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>